

local Players = game:GetService("Players")
local RS = game:GetService("RunService")
local VU = game:GetService("VirtualUser")
local GuiService = game:GetService("GuiService")
local lp = Players.LocalPlayer

-- [[ 1. ANTI-KICK SYSTEM (META-HOOK) ]] --
-- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô‡πÄ‡∏Å‡∏°‡∏™‡∏±‡πà‡∏á Kick ‡πÄ‡∏£‡∏≤‡∏≠‡∏≠‡∏Å (‡∏ï‡πâ‡∏≠‡∏á‡πÉ‡∏ä‡πâ Executor ‡∏ó‡∏µ‡πà‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö hookmetamethod)
if hookmetamethod then
    local old; old = hookmetamethod(game, "__namecall", function(self, ...)
        local method = getnamecallmethod()
        if not checkcaller() then
            if (method == "Kick" or method == "kick") and self == lp then
                warn("[Anti-AFK] üõ°Ô∏è Blocked Kick Attempt from Game!")
                return nil -- ‡∏ö‡∏•‡πá‡∏≠‡∏Å‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á Kick
            elseif method == "IsFocused" or method == "GetFocused" then
                return true -- ‡∏´‡∏•‡∏≠‡∏Å‡πÄ‡∏Å‡∏°‡∏ß‡πà‡∏≤‡πÄ‡∏£‡∏≤‡∏¢‡∏±‡∏á‡πÇ‡∏ü‡∏Å‡∏±‡∏™‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠‡∏≠‡∏¢‡∏π‡πà
            end
        end
        return old(self, ...)
    end)
end

-- [[ 2. VIRTUAL INPUT SIMULATION ]] --
-- ‡∏à‡∏≥‡∏•‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°‡πÅ‡∏•‡∏∞‡∏Ç‡∏¢‡∏±‡∏ö‡πÄ‡∏°‡∏≤‡∏™‡πå
task.spawn(function()
    while task.wait(math.random(60, 120)) do -- ‡∏ó‡∏≥‡∏ó‡∏∏‡∏Å‡πÜ 1-2 ‡∏ô‡∏≤‡∏ó‡∏µ
        pcall(function()
            VU:CaptureController()
            VU:SetKeyDown("W")
            task.wait(0.1)
            VU:SetKeyUp("W")
            -- ‡∏Ç‡∏¢‡∏±‡∏ö‡πÄ‡∏°‡∏≤‡∏™‡πå‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢
            VU:MouseMove(Vector2.new(math.random(10, 50), math.random(10, 50)))
        end)
    end
end)

-- [[ 3. PHYSICAL GHOST MOVEMENT ]] --
-- ‡∏Ç‡∏¢‡∏±‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏•‡πá‡∏Å‡∏ô‡πâ‡∏≠‡∏¢‡∏°‡∏≤‡∏Å (CFrame) ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ Server ‡πÄ‡∏´‡πá‡∏ô‡∏ß‡πà‡∏≤‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
task.spawn(function()
    while task.wait(math.random(15, 30)) do
        pcall(function()
            local root = lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")
            if root then
                -- ‡∏Ç‡∏¢‡∏±‡∏ö‡∏Ç‡∏∂‡πâ‡∏ô‡∏ô‡∏¥‡∏î‡∏ô‡∏∂‡∏á‡πÅ‡∏•‡πâ‡∏ß‡∏•‡∏á‡∏ó‡∏µ‡πà‡πÄ‡∏î‡∏¥‡∏° (‡∏ï‡∏≤‡πÄ‡∏õ‡∏•‡πà‡∏≤‡πÅ‡∏ó‡∏ö‡∏°‡∏≠‡∏á‡πÑ‡∏°‡πà‡πÄ‡∏´‡πá‡∏ô)
                root.CFrame = root.CFrame * CFrame.new(0, 0.05, 0)
                task.wait(0.1)
                root.CFrame = root.CFrame * CFrame.new(0, -0.05, 0)
            end
        end)
    end
end)

-- [[ 4. ROBLOX UI CLEANER ]] --
-- ‡∏Ñ‡∏≠‡∏¢‡∏•‡∏ö‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô "You have been disconnected for being idle" ‡∏Ç‡∏≠‡∏á Roblox (‡∏ñ‡πâ‡∏≤‡∏°‡∏±‡∏ô‡πÄ‡∏î‡πâ‡∏á‡∏Ç‡∏∂‡πâ‡∏ô‡∏°‡∏≤)
task.spawn(function()
    while task.wait(2) do
        pcall(function()
            for _, gui in pairs(lp.PlayerGui:GetDescendants()) do
                if gui:IsA("TextLabel") and (gui.Text:lower():find("20 minutes") or gui.Text:lower():find("idle")) then
                    if gui.Parent then gui.Parent:Destroy() end
                    print("[Anti-AFK] üóëÔ∏è Auto-removed Roblox Idle Warning")
                end
            end
        end)
    end
end)

-- [[ 5. STANDARD ANTI-IDLE ]] --
-- ‡∏ï‡∏±‡∏î‡∏Å‡∏≤‡∏£‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠‡∏™‡∏±‡∏ç‡∏ç‡∏≤‡∏ì Idled ‡∏Ç‡∏≠‡∏á Roblox
if getconnections then
    for _, v in pairs(getconnections(lp.Idled)) do
        v:Disable()
    end
else
    -- Fallback ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö Executor ‡∏ö‡∏≤‡∏á‡∏ï‡∏±‡∏ß
    lp.Idled:Connect(function()
        VU:CaptureController()
        VU:ClickButton2(Vector2.new())
    end)
end

-- ‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡πÉ‡∏ô F9 Console ‡∏ß‡πà‡∏≤‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß
warn("üëª JASON'S GHOST ANTI-AFK: ENABLED (Check F9 Console)")
print("‚úÖ Script is running in background. No UI will be shown.")
